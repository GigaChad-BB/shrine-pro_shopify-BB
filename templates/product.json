        "custom_liquid_yz9JWX": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<div class=\"product__block product__block--custom_liquid\">\n  <style>\n    #people-viewing{\n      position:relative; padding-left:25px; font-size:16px; font-weight:bold;\n    }\n    #people-viewing::before{\n      content:''; position:absolute; left:0; top:50%; transform:translateY(-50%);\n      width:12px; height:12px; border-radius:50%;\n      background-color:#e63946;\n      box-shadow:0 0 10px #f4af29, 0 0 20px #f4af29;\n      animation:glowing 2.5s infinite;\n    }\n    @keyframes glowing{\n      0%{ box-shadow:0 0 5px #f4af29, 0 0 10px #f4af29; }\n      50%{ box-shadow:0 0 20px #f4af29, 0 0 30px #f4af29; }\n      100%{ box-shadow:0 0 5px #f4af29, 0 0 10px #f4af29; }\n    }\n  </style>\n\n  <div id=\"people-viewing\" data-countdown=\"6\" data-min=\"4\" data-interval=\"5500\">\n  Plus que <span id=\"stock-count\">6</span> en stock\n  </div>\n\n  <script>\n  (function(){\n    // ---- helpers\n    function ready(fn){ if(document.readyState!=='loading'){fn();} else {document.addEventListener('DOMContentLoaded', fn);} }\n\n    function onceVisible(el, cb){\n      if(!el) return;\n\n      function inView(){\n        const r = el.getBoundingClientRect();\n        const vh = window.innerHeight || document.documentElement.clientHeight;\n        const vw = window.innerWidth  || document.documentElement.clientWidth;\n        return r.top < vh*0.9 && r.bottom > vh*0.1 && r.left < vw && r.right > 0;\n      }\n\n      if('IntersectionObserver' in window){\n        const io = new IntersectionObserver((entries)=>{\n          for(const e of entries){\n            if(e.isIntersecting){ io.disconnect(); cb(); return; }\n          }\n        }, { threshold:0.01, rootMargin:'0px 0px -20% 0px' });\n        io.observe(el);\n        // sÃ©curitÃ© au cas oÃ¹ IO ne tire pas\n        if(inView()){ io.disconnect(); cb(); }\n      }else{\n        const onScroll = ()=>{ if(inView()){ window.removeEventListener('scroll', onScroll); window.removeEventListener('resize', onScroll); cb(); } };\n        window.addEventListener('scroll', onScroll, {passive:true});\n        window.addEventListener('resize', onScroll);\n        onScroll();\n      }\n    }\n\n    function start(el){\n      if(!el || el.dataset.started==='1') return;\n      el.dataset.started='1';\n\n      const span = el.querySelector('#stock-count');\n      if(!span) return;\n\n      const START = parseInt(el.dataset.countdown || span.textContent, 10) || 4;\n      const MIN   = parseInt(el.dataset.min || '1', 10);\n      const STEP  = parseInt(el.dataset.interval || '5000', 10);\n\n      let val = START;\n      span.textContent = val;\n\n      // ---- mode: compte Ã  rebours (true) ou un seul changement (false)\n      const COUNTDOWN = true;\n\n      if(!COUNTDOWN){\n        setTimeout(()=>{ span.textContent = String(Math.max(MIN, val-1)); }, STEP);\n        return;\n      }\n\n      const tick = ()=>{\n        if(document.hidden) return; // saute le tick si lâ€™onglet est cachÃ©\n        if(val > MIN){\n          val--; span.textContent = String(val);\n          if(val === MIN) clearInterval(timer);\n        }\n      };\n      const timer = setInterval(tick, STEP);\n      // si tu veux dÃ©crÃ©menter une premiÃ¨re fois aprÃ¨s STEP, dÃ©commente:\n      // setTimeout(tick, STEP);\n    }\n\n    function boot(){\n      const el = document.getElementById('people-viewing');\n      if(!el) return;\n      onceVisible(el, ()=> start(el));\n    }\n\n    // DOM prÃªt + Ã©vÃ©nements Shopify (sections rechargÃ©es)\n    ready(boot);\n    document.addEventListener('shopify:section:load', boot);\n    document.addEventListener('shopify:section:select', boot);\n  })();\n  </script>\n</div>"
